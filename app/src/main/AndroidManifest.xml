<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="primeiroapp.studio.com.meuprimeiroapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/lg_bodybuilder"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/lg_bodybuilder"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="@string/login_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/todo_List" />
        <activity android:name=".CadActivity" />
        <activity
            android:name=".CadUsuarioActivity"
            android:label="@string/title_activity_cad_usuario">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ListUsuariosActivity" />
        </activity>
        <activity
            android:name=".ListUsuariosActivity"
            android:label="@string/usuarios">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".MenuActivityTreino"
            android:label="@string/title_activity_menu_treino">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".DiaTreinoActivity"
            android:label="@string/title_activity_dia_treino">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MenuActivityTreino" />
        </activity>
        <activity
            android:name=".CadTreinoActivity"
            android:label="@string/title_activity_cad_treino">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MenuActivityTreino" />
        </activity>
        <activity android:name=".CadMontActivity"
                  android:label="@string/title_activity_cad_mont">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MenuActivityTreino" />

        </activity>
    </application>

</manifest>